{
  "compilerOptions": {
    "baseUrl": "./",
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "strict": true,
    "outDir": "./dist",
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "paths": {
      "*": ["node_modules/*", "src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
  },
  "exclude": ["node_modules"],
}
